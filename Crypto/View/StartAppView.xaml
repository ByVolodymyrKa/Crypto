<Window x:Class="Crypto.View.StartAppView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Crypto.View"
        xmlns:viewmodel="clr-namespace:Crypto.ViewModel"
        mc:Ignorable="d"
        Title="StartAppView" Height="450" Width="800">
    <Window.DataContext>
        <viewmodel:AppVewModel/>
    </Window.DataContext>

    <Button Content="{Binding Text}" Width="200" Height="50" Command="{Binding ResetButtonCommand}"/>
    
</Window>
